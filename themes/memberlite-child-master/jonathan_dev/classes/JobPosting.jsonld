[{"Label":"applicantLocationRequirements","Comment":"The location(s) applicants can apply from. This is usually used for telecommuting jobs where the applicant does not need to be in a physical office. Note: This should not be used for citizenship or work visa requirements.","ExpectedTypes":"http:\/\/schema.org\/AdministrativeArea","FirstLevel":"True"},{"Label":"applicationContact","Comment":"Contact details for further information relevant to this job posting.","ExpectedTypes":"http:\/\/schema.org\/ContactPoint","FirstLevel":"True"},{"Label":"baseSalary","Comment":"The base salary of the job or of an employee in an EmployeeRole.","ExpectedTypes":"http:\/\/schema.org\/MonetaryAmount, http:\/\/schema.org\/Number, http:\/\/schema.org\/PriceSpecification","FirstLevel":"True"},{"Label":"datePosted","Comment":"Publication date of an online listing.","ExpectedTypes":"http:\/\/schema.org\/Date, http:\/\/schema.org\/DateTime","FirstLevel":"True"},{"Label":"educationRequirements","Comment":"Educational background needed for the position or Occupation.","ExpectedTypes":"http:\/\/schema.org\/EducationalOccupationalCredential, http:\/\/schema.org\/Text","FirstLevel":"True"},{"Label":"eligibilityToWorkRequirement","Comment":"The legal requirements such as citizenship, visa and other documentation required for an applicant to this job.","ExpectedTypes":"http:\/\/schema.org\/Text","FirstLevel":"True"},{"Label":"employerOverview","Comment":"A description of the employer, career opportunities and work environment for this position.","ExpectedTypes":"http:\/\/schema.org\/Text","FirstLevel":"True"},{"Label":"employmentType","Comment":"Type of employment (e.g. full-time, part-time, contract, temporary, seasonal, internship).","ExpectedTypes":"http:\/\/schema.org\/Text","FirstLevel":"True"},{"Label":"employmentUnit","Comment":"Indicates the department, unit and\/or facility where the employee reports and\/or in which the job is to be performed.","ExpectedTypes":"http:\/\/schema.org\/Org